<html>
	<head><title>Task master</title></head>
<body>
<h2>Tâches</h3>
<p>
	<ul>
		{{#each tasks}}
		<li><a href="/task/{{this.id}}/">{{this.name}}</a></li>
		{{/each}}
	</ul>
</p>



<h2>Actions</h2>
<p>
	<form action="/" method="post" >
		<input type="text" name="name"/>
		<input type="submit" value="créer" />
	</form>


</p>

<h3> Autres</h3>
<p><a href="/foods">food</a></p>

</body>
</html>





